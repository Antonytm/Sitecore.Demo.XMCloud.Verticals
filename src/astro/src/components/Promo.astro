---
import PromoDefault from "./PromoVariants/PromoDefault.astro";
import PromoWithText from "./PromoVariants/PromoWithText.astro";

const variantName = Astro.props.route.params.FieldNames ?? 'Default';
---
<>
  {variantName === 'Default' && <PromoDefault route={Astro.props.route} />}
  {variantName === 'WithText' && <PromoWithText route={Astro.props.route} />}
</>