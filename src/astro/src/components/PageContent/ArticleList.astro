---
import ArticleListDefault from  './ArticleListVariants/ArticleListDefault.astro'
import ArticleListThreeColumn from  './ArticleListVariants/ArticleListThreeColumn.astro'
import ArticleListSimplified from  './ArticleListVariants/ArticleListSimplified.astro'

const variantName = Astro.props.route.params.FieldNames ?? 'Default';

console.log(Astro.props.route.params.FieldNames);
---
<>
  {Astro.props.route.params.FieldNames === 'Default' && <ArticleListDefault route={Astro.props.route} />}
  {Astro.props.route.params.FieldNames === 'ThreeColumn' && <ArticleListThreeColumn route={Astro.props.route} />}
  {Astro.props.route.params.FieldNames === 'Simplified' && <ArticleListSimplified route={Astro.props.route} />}
</>