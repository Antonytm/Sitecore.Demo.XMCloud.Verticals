---
import ImageBanner from "./ImageVariants/ImageBanner.astro";
import ImageDefault from "./ImageVariants/ImageDefault.astro";

const variantName = Astro.props.route.params.FieldNames ?? 'Default';
---
<>
  {variantName === 'Default' && <ImageDefault route={Astro.props.route} />}
  {variantName === 'Banner' && <ImageBanner route={Astro.props.route} />}
</>